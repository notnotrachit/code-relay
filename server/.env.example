# AI Proxy Server Configuration

# Server ports
HTTP_PORT=3000
WS_PORT=3001

# Request timeout (milliseconds)
REQUEST_TIMEOUT=30000

# CORS settings
CORS_ORIGIN=*

# Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Extension connection settings
MAX_RECONNECT_ATTEMPTS=5
RECONNECT_DELAY=5000